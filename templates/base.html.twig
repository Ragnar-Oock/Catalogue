<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Accueil{% endblock %} - Catalogue</title>
        {# {{ encore_entry_link_tags('app.scss') }} #}
        {{ encore_entry_link_tags('main') }}
        {% block stylesheets %}{% endblock %}
    </head>
    <body>
        <header class="header">
            <img src="{{ asset('medias/img/logo.png') }}" alt="logo" class="header_logo">
        </header>
        <main>
            {% block main %}{% endblock %}
        </main>
        {{ encore_entry_script_tags('main') }}
        {% block javascripts %}{% endblock %}
    </body>
</html>
